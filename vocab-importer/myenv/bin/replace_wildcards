import re
import sys

from fastcore.py2pyi import replace_wildcards

if __name__ == "__main__":
    sys.argv[0] = re.sub(r"(-script\.pyw|\.exe)?$", "", sys.argv[0])
    sys.exit(replace_wildcards())
